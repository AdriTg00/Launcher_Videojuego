     <property name="styleSheet">
      <string notr="true">/* --- QSLIDER HORIZONTAL --- */
QSlider::groove:horizontal {
    border: 2px solid #C9A43A;              /* Contorno dorado */
    height: 10px;
    background: #F4E29B;                    /* Amarillo suave (igual que botones) */
    border-radius: 5px;
    margin: 0px 10px;
}

QSlider::handle:horizontal {
    background: qradialgradient(
        cx: 0.4, cy: 0.4,
        fx: 0.4, fy: 0.4,
        radius: 0.6,
        stop: 0 #FFE873,
        stop: 1 #C9A43A
    );
    border: 2px solid #5A4A00;
    width: 20px;
    height: 20px;
    border-radius: 10px;
    margin: -6px 0;
}

QSlider::handle:horizontal:hover {
    background: qradialgradient(
        cx: 0.4, cy: 0.4,
        fx: 0.4, fy: 0.4,
        radius: 0.6,
        stop: 0 #FFF3B0,
        stop: 1 #E5B937
    );
}

/* --- PARTE IZQUIERDA (LLENA) --- */
QSlider::sub-page:horizontal {
    background: #9BCB43;                    /* Verde suave tipo “Cargar datos” */
    border: 2px solid #5F8A2A;
    border-radius: 5px;
}

/* --- PARTE DERECHA (VACÍA) --- */
QSlider::add-page:horizontal {
    background: #EDE2AC;                    /* Beige claro */
    border: 2px solid #C9A43A;
    border-radius: 5px;
}

/* --- QSLIDER VERTICAL (por si lo usas) --- */
QSlider::groove:vertical {
    border: 2px solid #C9A43A;
    width: 10px;
    background: #F4E29B;
    border-radius: 5px;
    margin: 10px 0px;
}

QSlider::handle:vertical {
    background: qradialgradient(
        cx: 0.4, cy: 0.4,
        fx: 0.4, fy: 0.4,
        radius: 0.6,
        stop: 0 #FFE873,
        stop: 1 #C9A43A
    );
    border: 2px solid #5A4A00;
    height: 20px;
    width: 20px;
    border-radius: 10px;
    margin: 0 -6px;
}

QSlider::sub-page:vertical {
    background: #9BCB43;
    border: 2px solid #5F8A2A;
    border-radius: 5px;
}

QSlider::add-page:vertical {
    background: #EDE2AC;
    border: 2px solid #C9A43A;
    border-radius: 5px;
}
</string>
