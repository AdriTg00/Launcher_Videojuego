    <widget class="QCheckBox" name="ventana">
     <property name="styleSheet">
      <string notr="true">/* --- ESTILO GENERAL DE QCheckBox --- */
QCheckBox {
    spacing: 8px;
    font-size: 10px;
    color: #3B2E05;
    padding: 4px;
}

/* --- CHECKBOX SIN MARCAR --- */
QCheckBox::indicator {
    width: 16px;
    height: 16px;
    border: 2px solid #C9A43A;        /* Borde dorado */
    border-radius: 4px;
    background-color: #F4E29B;        /* Amarillo suave */
}

/* --- CHECKBOX MARCADO --- */
QCheckBox::indicator:checked {
    image: none;
    background-color: #9BCB43;        /* Verde tipo “Cargar datos” */
    border: 2px solid #5F8A2A;
    border-radius: 4px;
}

/* --- CHECKBOX AL PASAR EL RATÓN --- */
QCheckBox::indicator:hover {
    background-color: #F9D95C;        /* Amarillo más brillante */
    border: 2px solid #E5B937;
}

/* --- CHECKBOX DESHABILITADO --- */
QCheckBox::indicator:disabled {
    background-color: #EDE2AC;
    border: 2px solid #B8A34F;
    opacity: 0.6;
}

QCheckBox:disabled {
    color: #8E8352;
}
</string>
