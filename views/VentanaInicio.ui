    <property name="styleSheet">
     <string notr="true">/* --- ComboBox principal --- */
QComboBox {
    font-family: &quot;Arial&quot;;
    font-size: 14px;
    font-weight: bold;
    padding: 6px 30px 6px 12px;
    min-height: 36px;
    border-radius: 10px;
    background: qlineargradient(
        x1: 0, y1: 0, x2: 0, y2: 1,
        stop: 0 rgba(246, 226, 143, 255),
        stop: 1 rgba(236, 198, 70, 255)
    );
    color: #2b2b2b;              /* texto oscuro, visible */
    border: 2px solid rgba(150, 110, 30, 0.6);
}

/* --- Flecha de desplegable --- */
QComboBox::drop-down {
    subcontrol-origin: padding;
    subcontrol-position: top right;
    width: 36px;
    border: none;
    background: transparent;
}

QComboBox::down-arrow {
    width: 0;
    height: 0;
    margin-right: 10px;
    border-left: 6px solid transparent;
    border-right: 6px solid transparent;
    border-top: 8px solid rgba(70, 40, 10, 0.9); /* color de la flecha */
}

/* --- Cuando el combo está presionado --- */
QComboBox:on {
    background: qlineargradient(
        x1: 0, y1: 0, x2: 0, y2: 1,
        stop: 0 rgba(240, 210, 110, 255),
        stop: 1 rgba(230, 180, 60, 255)
    );
}

/* --- Lista desplegable --- */
QComboBox QAbstractItemView {
    outline: 0;
    background: #fff9e6; /* tono beige claro */
    color: #2b2b2b;       /* texto oscuro */
    border: 1px solid rgba(150, 110, 30, 0.6);
    padding: 6px;
    border-radius: 6px;
    selection-background-color: rgba(245, 234, 180, 255);
    selection-color: black; /* texto visible al seleccionar */
}

/* --- Items de la lista --- */
QComboBox::item {
    color: #2b2b2b;
    background: transparent;
    padding: 4px 10px;
}

QComboBox::item:selected {
    background: rgba(245, 234, 180, 255);
    color: black;
}
</string>
